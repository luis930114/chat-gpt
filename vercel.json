{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/api/completions", "dest": "/api/server.js" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}


